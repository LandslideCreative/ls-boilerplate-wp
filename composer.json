{
    "name": "landslidecreative/ls-boilerplate-wp",
    "authors": [
        {
            "name": "Landslide Creative",
            "email": "info@landslidecreative.com"
        }
    ],
    "repositories" : [
          {
               "type" : "composer",
               "url"  : "https://wpackagist.org"
          },
          {
               "type" : "vcs",
               "url" : "git@github.com:jsjenkins/advanced-custom-fields-pro.git"
          },
          {
               "type" : "vcs",
               "url" : "git@github.com:jsjenkins/gravityforms.git"
          },
          {
               "type" : "vcs",
               "url" : "git@github.com:jsjenkins/wp-rocket.git"
          },
          {
               "type" : "vcs",
               "url" : "git@github.com:jsjenkins/wp-migrate-db-pro.git"
          }
     ],
     "extra" : {
          "installer-paths" : {
               "wp-content/plugins/{$name}/" : ["type:wordpress-plugin"],
               "wp-content/themes/{$name}/" : ["type:wordpress-theme"]
          }
     },
    "minimum-stability": "dev",
    "prefer-stable" : true,
    "require": {
          "landslidecreative/advanced-custom-fields-pro" : "5.8.7",
          "wpackagist-plugin/classic-editor" : "1.5",
          "landslidecreative/gravityforms" : "2.4.16.3",
          "wpackagist-plugin/imagify" : "1.9.8.1",
          "wpackagist-plugin/intuitive-custom-post-order" : "3.1.2",
          "wpackagist-plugin/wordfence" : "7.4.2",
          "landslidecreative/wp-migrate-db-pro" : "1.9.8",
          "landslidecreative/wp-rocket" : "3.4.3",
          "wpackagist-plugin/wordpress-seo" : "12.7.1"
    }
}
