{
    "name": "landslidecreative/ls-boilerplate-wp",
    "authors": [
        {
            "name": "Landslide Creative",
            "email": "info@landslidecreative.com"
        }
    ],
    "repositories" : [
          {
               "type" : "composer",
               "url"  : "https://wpackagist.org"
          },
          {
               "type" : "vcs",
               "url" : "git@github.com:jsjenkins/advanced-custom-fields-pro.git"
          },
          {
               "type" : "vcs",
               "url" : "git@github.com:jsjenkins/gravityforms.git"
          },
          {
               "type" : "vcs",
               "url" : "git@github.com:jsjenkins/wp-rocket.git"
          },
          {
               "type" : "vcs",
               "url" : "git@github.com:jsjenkins/backupbuddy.git"
          }
     ],
     "extra" : {
          "installer-paths" : {
               "wp-content/plugins/{$name}/" : ["type:wordpress-plugin"],
               "wp-content/themes/{$name}/" : ["type:wordpress-theme"]
          }
     },
    "minimum-stability": "dev",
    "prefer-stable" : true,
    "require": {
         "wpackagist-plugin/intuitive-custom-post-order" : "3.1.1",
         "wpackagist-plugin/wordpress-seo" : "7.3",
         "wpackagist-plugin/wordfence" : "7.1.3",
         "wpackagist-plugin/wp-migrate-db" : "1.0.2",
         "wpackagist-plugin/imagify" : "1.7.1.3",
         "landslidecreative/advanced-custom-fields-pro" : "5.6.10",
         "landslidecreative/gravityforms" : "2.3.0.4",
         "landslidecreative/wp-rocket" : "3.0.4",
         "landslidecreative/backupbuddy" : "8.2.3.5"
    }
}
